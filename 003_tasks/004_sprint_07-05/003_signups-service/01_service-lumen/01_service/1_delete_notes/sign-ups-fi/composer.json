{
    "name": "ctdesarrollo/sign-ups-service",
    "description": "Students Sign-ups & Updates Microservice",
    "keywords": ["framework", "laravel", "lumen", "microservice", "ddd"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.1",
        "beberlei/assert": "^3.2",
        "elasticsearch/elasticsearch": "^7.6",
        "enqueue/amqp-bunny": "^0.10.1",
        "enqueue/laravel-queue": "^0.10.2",
        "enqueue/simple-client": "^0.10.1",
        "guzzlehttp/guzzle": "^6.5",
        "laravel/lumen-framework": "^9.0",
        "league/fractal": "^0.19.2",
        "league/tactician": "^1.0",
        "league/uri": "^6.2",
        "myclabs/php-enum": "^1.7",
        "neomerx/json-api": "^4.0",
        "nyholm/psr7": "^1.2",
        "php-amqplib/php-amqplib": ">=3.0",
        "php-amqplib/rabbitmq-bundle": "^2.11",
        "rnevarezc/ddd-legos": "^1.3",
        "spatie/data-transfer-object": "^2.8.4",
        "appzcoder/lumen-routes-list": "^1.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Application\\": "src/Application/",
            "Domain\\": "src/Domain/",
            "Infrastructure\\": "src/Infrastructure/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
